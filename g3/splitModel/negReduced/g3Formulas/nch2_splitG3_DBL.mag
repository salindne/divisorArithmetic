            //M2  := ExactQuotient(s*(v + v + h) - k, u);
            //up := s^2 + M2;
            //upp  := up/LeadingCoefficient(up);
            t0 := u2*s0;
            upp0 := w2*d^2*(t7 + t0 + (s0^2 - k3)/2);

            //vpp  := -V - h  + (V - s*u - v) mod upp;
            t3 := upp0*(T2 + s0) - yn2 - v2 - t0 - T1;
            vpp0 := upp0*(yn1 + v1 + s0*u1  + T0 + upp0*t3) - v0 - s0*u0;

            if DBL_DEBUG then "DBL44"; end if;
            return 0,0,1,upp0,yn2,yn1,vpp0,0;
        end if;//w0

        w1 := w0*sp1;           //s1*(s1 - c4)*D
        w2 := (w1*d)^-1;        //1/s1*(s1 - c4)*d^3
        w3 := w2*w1;            //1/d
        w4 := w2*d^2*d;         //1/s1*(s1 - c4)
        s1 := sp1*w3;
        s0 := sp0*w3;

        //M2  := ExactQuotient(s*(v + v + h) - k, u);
        //up := s^2 + M2;
        //upp  := up/LeadingCoefficient(up);
        t2 := s0 + T2;
        upp1 := w4*(s1*(t2 + s0) - s0 - s0);
        upp0 := w4*(s1*t7 + s0*t2 - k3);
        
        //vpp  := -V - h  + (V - s*u - v) mod upp;
        t1 := s1 - 2;
