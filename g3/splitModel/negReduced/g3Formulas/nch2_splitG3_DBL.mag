        t4 := ud0*k4;                   
        k3 := m7*(t1 + t1) - t3;                                  //m7
        k2 := w2*(t0 + t0 - v2^2 - d4) - ud0*k4 - ud1*k3;   //m7^2
        k1 := w3*(f3 - vh1*v2) - ud0*k3 - ud1*k2;      //m7^3
        k0 := w4*(f2 - v1^2 - v0*vh2) - ud0*k2 - ud1*k1;      //m7^4
        t5 := k3 - t3;                                                  //m7
