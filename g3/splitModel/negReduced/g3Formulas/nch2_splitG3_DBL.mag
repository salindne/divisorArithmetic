                w4 := w3*d;     //q1
                M20 := k2*w4;
                
                upp0 := w0*w3;
                upp2 := upp0^2;
                vpp0 := M20 - v0 + upp0*(yn1 + v1 - upp0*(yn2 + v2 - upp2 - upp2));

                if DBL_DEBUG then "DBL46"; end if;
                return 0,0,1,upp0,yn2,yn1,vpp0,2;
            end if;//k3

            //M2  := ExactQuotient(r*(v + v + h) + k*q, u);
            //up := R2 - q*M2;
            //upp  := up/LeadingCoefficient(up);
            w3 := (k3*sp2)^-1;  //1/s2*k3*d
            w4 := w3*k3;        //1/s2*d
            w5 := w3*sp2*k2;    //k2/k3
            w6 := sp1*w4;    //s1/s2
            w7 := w4*d;      //1/s2            
            t1 := w5 - w6;    //M20/M21
            t2 := u2 - w6;
            upp1 := t1 + t2;
            upp0 := t1*t2;

            //vpp := -V - h + (M2 - v + V) mod upp;
            t0 := -upp1 - upp1;
            t1 := t0*upp1;
            t2 := -yn2 - v2 - upp0 - upp0 - t1;
            t3 := t2*upp0;
            t4 := w7*k3;
