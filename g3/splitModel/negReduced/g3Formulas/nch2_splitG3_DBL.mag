            t0 := d2*m7;
            w6 := (w5*t0)^-1;           //1/d2^2*m7*(s0*c4 + k4)
            t1 := w6*d2;                //1/d2*m7*(s0*c4 + k4)
            w7 := t1*t0;                //1/(s0*c4 + k4)
            w8 := t1*w5;                //1/m7
            w9 := w8^2;                 //1/m7^2
            up1 := ud1*w8;
            up0 := ud0*w9;
            k3 := k3*w8;
            k2 := k2*w9;
            s0 := sp0*w6*w5*m7;         
