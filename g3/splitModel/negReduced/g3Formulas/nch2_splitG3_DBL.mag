            //up := s^2 + M2;
            //upp  := up/LeadingCoefficient(up);
            upp0 := (k3*w3 - s0)/2 - u2;

            //vpp  := -V - h  + (V - s*u - v) mod upp;
            t1 := yn2 + v2 + s0*u2 - upp0*(s0 + upp0 + upp0);
            vpp0 := upp0*(yn1 + v1 + s0*u1 - upp0*t1) - v0 - s0*u0;
            
            if DBL_DEBUG then "DBL42"; end if;
            return 0,0,1,upp0,yn2,yn1,vpp0,1;
        end if;//sp1
        
        w0 := sp1 - d - d;       //(s1 - c4)*d

        if IsZero(w0) then
