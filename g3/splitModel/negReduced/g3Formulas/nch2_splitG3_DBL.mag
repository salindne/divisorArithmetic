        upp0 := -v1 - t0*(s0*(vh2 + s0) - k2) - up0*M22 - up1*M21;
        
        //vpp := -V - h  + (-s*up - v + V) mod upp;
        t0   := -s1 - upp2 - upp2;
        t1   := upp1 + upp1;
        t3   := s1*up1;
        t4   := s0*up0;
        vpp2 := -v2 - t3 - s0 - t1 - upp2*t0; 
