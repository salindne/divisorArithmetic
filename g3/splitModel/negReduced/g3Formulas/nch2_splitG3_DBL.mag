    */ //endIGNORE

    f1:= ccs[1][1][2];
    f2:= ccs[1][1][3];
    f3:= ccs[1][1][4];
    yn0:= ccs[1][2][1];
    yn1:= ccs[1][2][2];
    yn2:= ccs[1][2][3];
    d4:= ccs[1][3][5];    
    

    //s :=  (k/vt) mod u;
    // Original Setup T for (s*vt = k) mod u
    //|t1,t2,t3|
    //|t4,t5,t6|
    //|t7,t8,t9|
    //Inverted T up to factor of d; M = (T^-1)*Det(T)
    //| m1= t5*t9-t8*t6,  m2= t3*t8-t2*t9, m3= t2*t6-t3*t5 |
    //| m4= t6*t7-t4*t9,  m5= t1*t9-t3*t7, m6= t3*t4-t1*t6 |
    //| m7= t4*t8-t5*t7,  m8= t2*t7-t1*t8, m9= t1*t5-t2*t4 |

    //vt  := (2*v + h) mod u; vt0 = t1, vt1 = t2, vt2 = t7
    T2 := u2 + u2;
    T1 := u1 + u1;
    T0 := u0 + u0;    
    vh0 := v0 + v0;
    vh1 := v1 + v1;
    vh2 := v2 + v2;
    t1 := vh0 - u0*T2;
    t4 := vh1 + T0 - u1*T2;
    t7 := vh2 + T1 - u2*T2;
    t2 := -u0*t7;
    t5 := t1 - u1*t7;
    t8 := t4 - u2*t7;
    t3 := -u0*t8;
    t6 := t2 - u1*t8;
    t9 := t5 - u2*t8;

    //Determinant of T = t1*m1 + t2*m4 + t3*m7
    //First column of M corresponds to polynomial q = m7x^2 + m4x + m1 = d/u1 mod u2.
    m1 := t5*t9 - t6*t8;
    m4 := t6*t7 - t4*t9;
    m7 := t4*t8 - t5*t7;
    d := t1*m1 + t2*m4 + t3*m7; 

    if IsZero(d) then
        //gcd(h + 2*v1, u1) = (x + xp1)
        // xp1 = -m8*(m7^-1)
        m8 := t2*t7 - t1*t8; // 
        if IsZero(m7) then
            if IsZero(t7) then

                if DBL_DEBUG then "DBL33"; end if;
                return 0,0,0,1,yn2,yn1,yn0,1;
            end if;//t7

            //gcd(h + 2*v1, u1) = x^2 + (t4/t7)x + (t1/t7);
            //ud0 := -t8;                                     //t7

            //deg1double(x + ud0, v)
            //w1 := (2*v + h) mod ud;
            w2 := t7^2;
            w3 := w2*t7;
            w4 := w2^2;
            w5 := w4*t7;
            T8 := t8^2;
            w1 := w4*vh0 + t8*(vh1*w3 + t8*(vh2*w2 - T8 - T8));  //t7^4

            //k := ExactQuotient(f - v*h - v^2, ud);
            //kp := k mod ud;        
            t0 := v0 - yn0;
            t1 := v1 - yn1;
            t2 := v2 - yn2;
            k5 := t2 + t2; 
            t3 := t8*k5;        
            k4 := t7*(t1 + t1) + t3;                          //t7
            k3 := w2*(t0 + t0 - v2^2 - d4) + t8*k4;        //t7^2
            k2 := w3*(f3 - vh1*v2) + t8*k3;                 //t7^3
            k1 := w4*(f2 - v1^2 - v0*vh2) + t8*k2;          //t7^4
            k0 := w5*(f1 - v0*vh1) + t8*k1;                 //t7^5
            kp0 := k0 + t8*(k1 + t8*(k2 + t8*(k3 + t8*(k4 + t3)))); //t7^5
            
            //Solve system
            //s   := R! (Q!kp*(Q!w1^-1));
            w6 := (w1*t7)^-1;                   //1/vt1*t7^5
            w7 := w6*w1;                        //1/t7
            up0 := -t8*w7;
            s0 := kp0*w6;

            //up := u^2;
            upp1 := up0 + up0;
            upp0 := up0^2;

            //vpp  := -V - h  + (s*up + v + V + h) mod upp;
            t0 := v2 - yn2;
            vpp1 := v1 + s0 - upp1*t0;
            vpp0 := v0 + up0*s0 - upp0*t0;

            if DBL_DEBUG then "DBL34"; end if;
