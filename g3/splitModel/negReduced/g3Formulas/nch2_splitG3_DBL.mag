            if IsZero(w1) then

                if DBL_DEBUG then "DBL43"; end if;
                return 0,0,0,1,yn2,yn1,yn0,0;
            end if;//w1

            w2 := (d*w1)^-1;    //1/(s0 + T2)*d^2
