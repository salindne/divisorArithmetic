        vpp0 := -v0 - t4 - upp0*t0;

        if DBL_DEBUG then "DBL40"; end if;
        return 1,upp2,upp1,upp0,vpp2,vpp1,vpp0,0;
    end if;//d

    //k := ExactQuotient(f - v*h - v^2, u);
    //kp := k mod u;
    t0  := v0 - yn0;
    t1  := v1 - yn1;
    T4  := v2 - yn2;
