        vpp1 := -v1 - (s1 + s0)*(u0 + u1) + t2 + t7 - (upp0 + upp1)*(t3 + t5) + t4 + t6;
        vpp0 := -v0 - t7 - t6;

        if DBL_DEBUG then "DBL45"; end if;
        return 0,1,upp1,upp0,yn2,vpp1,vpp0,0;
    end if;//sp2

    //q   := u2 div s;
