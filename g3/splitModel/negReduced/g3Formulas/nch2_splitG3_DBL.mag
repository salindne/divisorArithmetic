                //vpp  := -V - h  + (-v + V) mod upp;
                t0 := -upp1 - upp1;
                t1 := upp1*t0;
                t2 := -yn2 - v2 - upp0 - upp0 - t1;
                t3 := upp0*t2;
                vpp1 := -v1 - (upp0 + upp1)*(t0 + t2) + t1 + t3;
                vpp0 := -v0 - t3;

                if DBL_DEBUG then "DBL36"; end if;
