        end if;//m7

        w2 := m7^2;
        w3 := w2*m7;
        w4 := w3*m7;
        W  := w4^2;
        ud1 := u2*m7 + m8;      //with weight m7
        ud0 := u1*w2 + m8*ud1;  //with weight m7^2

        //deg2double(ud, v)
        //vt  := (2*v + h) mod ud;
        t0  := ud1 + ud1;                               //m7
        t1  := ud1*t0;                                  //m7^2
        t2  := ud0 + ud0 + vh2*w2 - t1;                 //m7^2
        t3  := ud0*t2;                                  //m7^4
        m3 := -vh1*w3 + ud1*t2 + ud0*t0;                //m7^3
        m4 := w4*vh0 - t3;                              //m7^4
        m1 := m4 + m3*ud1;                              //m7^4
        m2 := -m3*ud0;                                  //m7^5
        d2  := m4*m1 - m2*m3;                           //W = m7^8

        //k := ExactQuotient(f - v*h - v^2, ud);
        //kp := k mod ud;        
        t0 := v0 - yn0;
        t1 := v1 - yn1;
        t2 := v2 - yn2;
        k4 := t2 + t2; 
