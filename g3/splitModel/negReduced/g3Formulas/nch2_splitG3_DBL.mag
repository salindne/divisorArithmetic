        t8 := ud0*t7;                                                   //m7^4
        kp1 := k1 - (ud0 + ud1)*(t5 + t7) + t6 + t8;                    //m7^3
        kp0 := k0 - t8;                                                 //m7^4
        
        //Solve system
        //s   := R! (Q!kp*(Q!d2^-1));
        sp0 := kp0*m1 + kp1*m2;                             //d2
        sp1 := kp0*m3 + kp1*m4;                             //d2/m7
        
        if IsZero(sp1) then
            if IsZero(sp0) then
                if IsZero(k4) then
                    //M2   := ExactQuotient(-k, u);
                    //upp  := M2/LeadingCoefficient(M2);
                    w5 := (k3*m7)^-1;                       //1/k3*w2 = k3*m7^2
                    upp0 := k2*w5 - ud1*w5*k3;
                    
                    //vpp  := -V - h  + (-v + V) mod upp;
                    upp2 := upp0^2;
